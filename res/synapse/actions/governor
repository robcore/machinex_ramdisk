UKM=/data/.machinex;
BB=busybox;

refresh () {
	 su -c "am startservice -n com.af.synapse/.lib.ActionService -e json '{ SDirectoryParser: { id:"/sys/devices/system/cpu/cpu0/cpufreq/"}}' &> /dev/null;"
}

if [ -f $1 ]; then
	OLD=`cat $1`
	if [[ ! -z $2 ]]; then
		$BB echo $2 > $1
	fi

	NEW=`$BB cat $1`

	if [ $OLD != $NEW ]; then
		refresh &
	fi

	$BB echo $NEW
fi
