case $(basename $0) in
cpuvolt)
	P=/sys/devices/system/cpu/cpu0/cpufreq/UV_uV_table
	;;
gpuvolt)
	P=/sys/devices/system/cpu/cpu0/cpufreq/GPU_volt_table
	;;
intvolt)
        P=/sys/class/devfreq/exynos5-busfreq-int/volt_table
        ;;
mifvolt)
        P=/sys/class/devfreq/exynos5-busfreq-mif/volt_table
        ;;
esac

readValue() {
	while read FREQ UVOLT; do
		if [ $FREQ == $1 ]; then
			echo $UVOLT
			return
		fi
	done < $P
}

writeValue() {
	VOLTARR=""
	while read FREQ UVOLT; do
		if [ $FREQ == $1 ]; then
			VOLTARR="$VOLTARR $2"
		else
			VOLTARR="$VOLTARR $UVOLT"
		fi
	done < $P

	echo $VOLTARR > $P
}

if [[ ! -z $2 ]]; then
	writeValue $1 $2
fi

echo $(readValue $1)
